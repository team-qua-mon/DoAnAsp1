@model IEnumerable<DoAnAsp.Areas.ADmin.Models.PhanQuyenModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenQuyen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChiTiet)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenQuyen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChiTiet)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MAQuyen">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MAQuyen">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MAQuyen">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
